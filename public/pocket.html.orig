             function addItemInCart(code){
               var newRow = new Object();
                   newRow["CODE"] = code;
                   newRow["PRESENTATION_NAME"] = produtos[code].PRESENTATION_NAME;
                   newRow["VALUE"] = produtos[code].VALUE;
                   
