                 $http.post("/registrar_usuario",{"usuario":usuario}).then(function (response) {
                     // body...
                     console.log(response);
                 }, function (response) {
                     console.log(response);
                     // body...
                 });
              }
              
              cart.total = carrinho_local();
              cart_initCart (); 
          
          });
          
          
          
